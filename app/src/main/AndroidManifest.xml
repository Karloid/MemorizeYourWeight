<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.krld.memorize" >
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/sql_icon"
        android:label="@string/app_name"
        android:name="com.krld.memorize.Application"
         >
        <activity android:name=".EditorActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".MenuActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <meta-data android:name="AA_DB_NAME" android:value="database2.db" />
        <meta-data android:name="AA_DB_VERSION" android:value="2" />
        <meta-data
            android:name="AA_MODELS"
            android:value="com.krld.memorize.models.Measurement" />
    </application>

</manifest>
